{
  "bindings": [
    {
      "name": "blobContents",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%DATA_SOURCES_CONTAINER%/internal-users.json",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "validUsers",
      "type": "blob",
      "direction": "out",
      "path": "%DATA_IMPORT_CONTAINER%/valid-users.json",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "errorUsers",
      "type": "blob",
      "direction": "out",
      "path": "%DATA_IMPORT_CONTAINER%/error-users.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
