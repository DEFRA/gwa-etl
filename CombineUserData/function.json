{
  "bindings": [
    {
      "name": "myBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%DATA_EXTRACT_CONTAINER%/{triggerFilename}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "triggerFileContents",
      "type": "blob",
      "direction": "in",
      "path": "%DATA_EXTRACT_CONTAINER%/{triggerFilename}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "internalUsers",
      "type": "blob",
      "direction": "out",
      "path": "%DATA_SOURCES_CONTAINER%/internal-users.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
